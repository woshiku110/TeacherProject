.page{
    display: flex;
    height: 100%;
    width: 100%;
    position: absolute;
    background-color: white;
    flex-direction: column;
}
.top_title{
    display: flex;
    width: 100%;
    height: 90rpx;
    background: #F0EFF4;
    align-items: center;
}
.top_title_line{
    display: flex;
    width:100%;
    height: 65rpx;
    border-left: solid 4rpx #fe9c75;
    background-color: #F0EFF4;
    align-items: center;
}
.top_title_line_label{
    margin-left: 20rpx;
}
.bottom_bt{
    width: 90%;
    height: 80rpx;
    display:flex;
    background-color: #fe9c75;
    position: fixed;
    top:calc(100% - 80rpx - 30rpx);
    border-radius: 10rpx; 
    left: 5%;
    color: white;
    justify-content: center;
    align-items: center;
}